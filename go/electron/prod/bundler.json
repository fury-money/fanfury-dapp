{
  "app_name": "Teritori",
  "icon_path_darwin": "./resources/icon.icns",
  "icon_path_linux": "./resources/icon.png",
  "icon_path_windows": "./resources/icon.ico",
  "version_electron": "26.2.4",
  "version_astilectron": "0.57.0",
  "info_plist": {
    "CFBundlePackageType": "APPL",
    "CFBundleInfoDictionaryVersion": "6.0",
    "CFBundleIconFile": "./resources/icon.icns",
    "CFBundleDisplayName": "Teritori",
    "CFBundleIdentifier": "com.teritori.app",
    "LSUIElement": "NO",
    "LSMinimumSystemVersion": "10.11",
    "NSHighResolutionCapable": true,
    "NSAppTransportSecurity": {
      "NSAllowsArbitraryLoads": true
    }
  },
  "environments": [
    {
      "arch": "amd64",
      "os": "darwin"
    },
    {
      "arch": "amd64",
      "os": "linux"
    },
    {
      "arch": "amd64",
      "os": "windows",
      "env": {
        "CC": "/usr/local/opt/mingw-w64/bin/x86_64-w64-mingw32-gcc",
        "CGO_ENABLED": "1"
      }
    }
  ]
}
